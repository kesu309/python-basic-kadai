      "source": [
        "# 消費税を加えた計算結果を返す関数\n",
        "def calculate_total_price(price, tax_rate):\n",
        "    total_price = price * (1 + tax_rate / 100)\n",
        "    return total_price\n",
        "\n",
        "# 商品の価格\n",
        "price = 10000\n",
        "\n",
        "# 消費税\n",
        "tax_rate = 10\n",
        "\n",
        "# 関数を呼び出して消費税込みの金額を計算\n",
        "total_price = calculate_total_price(price, tax_rate)\n",
        "\n",
        "# 結果を表示\n",
        "print(total_price)\n"
      ]
